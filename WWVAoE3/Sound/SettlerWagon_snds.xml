<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="SettlerWagon">
		<soundtype name="Select">
			<soundset name="GermanSettlerMaleSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="CavalryGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="SettlerBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="GermanSettlerMaleAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="GermanSettlerMaleAttack"/>
				</choice>
				<choice name="build">
					<soundset name="GermanSettlerMaleBuild"/>
				</choice>
				<choice name="Huntable">
					<soundset name="GermanSettlerMaleGatherMeat"/>
				</choice>
				<choice name="Tree">
					<soundset name="GermanSettlerMaleGatherWood"/>
				</choice>
				<choice name="AbstractMine">
					<soundset name="GermanSettlerMaleGatherCoin"/>
				</choice>
				<choice name="AbstractFarm">
					<soundset name="GermanSettlerMaleFarm"/>
				</choice>
				<choice name="AbstractResourceCrate">
					<soundset name="GermanSettlerMaleAcknowledge"/>
				</choice>
				<choice name="AbstractFruit">
					<soundset name="GermanSettlerMaleGatherFruit"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>