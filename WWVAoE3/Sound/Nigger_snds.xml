<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="Nigger">
		<soundtype name="Select">
			<soundset name="NiggerSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="SettlerBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="NiggerAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="NiggerAttack"/>
				</choice>
				<choice name="Huntable">
					<soundset name="NiggerGatherMeat"/>
				</choice>
				<choice name="Tree">
					<soundset name="NiggerGatherWood"/>
				</choice>
				<choice name="AbstractMine">
					<soundset name="NiggerGatherCoin"/>
				</choice>
				<choice name="AbstractFarm">
					<soundset name="NiggerFarm"/>
				</choice>
				<choice name="AbstractResourceCrate">
					<soundset name="NiggerAcknowledge"/>
				</choice>
				<choice name="AbstractFruit">
					<soundset name="NiggerGatherFruit"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>