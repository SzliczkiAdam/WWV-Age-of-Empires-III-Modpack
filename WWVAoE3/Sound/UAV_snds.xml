<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="UAV">
		<soundtype name="Select">
			<soundset name="UAVSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="UAVGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="UAVDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="UAVBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="UAVMove"/>
				</choice>
				<choice name="enemy">
					<soundset name="UAVAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>