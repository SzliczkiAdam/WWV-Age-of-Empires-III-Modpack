<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="ypEggHotDogCart">
		<soundtype name="Select">
			<soundset name="SvabTelepesSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MilitaryBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="SvabTelepesMaleAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="SvabTelepesMaleAttack"/>
				</choice>
				<choice name="build">
					<soundset name="SvabTelepesMaleBuild"/>
				</choice>
				<choice name="Huntable">
					<soundset name="SvabTelepesMaleGatherMeat"/>
				</choice>
				<choice name="Tree">
					<soundset name="SvabTelepesMaleGatherWood"/>
				</choice>
				<choice name="AbstractMine">
					<soundset name="SvabTelepesMaleGatherCoin"/>
				</choice>
				<choice name="AbstractFarm">
					<soundset name="SvabTelepesMaleFarm"/>
				</choice>
				<choice name="AbstractResourceCrate">
					<soundset name="SvabTelepesMaleAcknowledge"/>
				</choice>
				<choice name="AbstractFruit">
					<soundset name="SvabTelepesMaleGatherFruit"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>